{
  "manifest_version": 3,
  "name": "Orbit",
  "version": "1.0",
  "description": "Minimal TUI-style startpage for the browser.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://suggestqueries.google.com/"
  ],
  "icons": {
    "128": "icon.png"
  }
}